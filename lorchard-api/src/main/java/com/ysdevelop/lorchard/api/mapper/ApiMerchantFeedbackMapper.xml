<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.ysdevelop.lorchard.api.mapper.ApiMerchantFeedbackDao">
	<insert id="add" parameterType="com.ysdevelop.lorchard.api.entity.MerchantFeedbackVo">
		insert into t_lorchard_mall_merchant_feedback
			(`merchantId`,`memberId`,`feedbackType`,`message`,`mobile`,`creatTime`)
		values
			(#{merchantId},#{memberId},#{feedbackType},#{message},#{mobile},now())
	</insert>
</mapper>