<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.ysdevelop.lorchard.api.mapper.ApiMerchantDao">
	<select id="getById"
		resultType="com.ysdevelop.lorchard.api.entity.MerchantVo">
		select 
			`merchantId`,`name`,`shopPrompt`,`shopLogo`,`shopDelivery`
		from 
			t_lorchard_mall_merchant
		where
			`merchantId`=#{merchantId}
	</select>
</mapper>