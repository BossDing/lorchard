<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.ysdevelop.lorchard.api.mapper.ApiMerchantBannerDao">
	<select id="list"
		resultType="com.ysdevelop.lorchard.api.entity.MerchantBannerVo">
		select 
			`merchantId`,`goodId`,`imagePath` 
		from 
			t_lorchard_mall_merchant_banner
		where
			`merchantId`=#{merchantId}
	</select>
</mapper>